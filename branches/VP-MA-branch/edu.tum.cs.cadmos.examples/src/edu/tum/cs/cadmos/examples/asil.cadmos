software component root {
	embed ArtificialD d
	embed leafB b
	
	property ASIL = 'D'
}
software component leafA {
	//overrides D from root
	property ASIL = 'B'
}
software component leafB {
	//overriden by inherited ASIL D
	property ASIL = 'C'
}



// leaf overrides parent!!


software component ArtificialD{
	embed leafA a1 // makes this ASIL D
	embed leafA a2
	embed CompareLogic c
	property ASIL = 'D'
}

software component CompareLogic{
	
}