<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="edu.tum.cs.cadmos.analysis.ui.views.architecture.ArchitectureViewPart"
            icon="icons/architecture_browser.gif"
            id="edu.tum.cs.cadmos.analysis.ui.views.architecture.ArchitectureViewPart"
            name="Architecture Browser">
      </view>
      <view
            class="edu.tum.cs.cadmos.analysis.ui.perspective.SchedulingAnalysisView"
            id="edu.tum.cs.cadmos.analysis.ui.perspective.SchedulingAnalysisView"
            name="Schedule Analysis">
      </view>
      <view
            class="edu.tum.cs.cadmos.analysis.ui.perspective.ConstraintView"
            id="edu.tum.cs.cadmos.analysis.ui.perspective.ConstraintView"
            name="Constraints">
      </view>
      <view
            class="edu.tum.cs.cadmos.analysis.ui.perspective.DebugIOView"
            id="edu.tum.cs.cadmos.analysis.ui.perspective.DebugIO"
            name="Debug and Z3 IO Console">
      </view>
      <view
            class="edu.tum.cs.cadmos.analysis.ui.perspective.Test"
            id="edu.tum.cs.cadmos.analysis.ui.perspective.Test"
            name="New ViewPart">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="edu.tum.cs.cadmos.analysis.ui.perspective.SchedulingAnalysisPerspective"
            id="edu.tum.cs.cadmos.analysis.ui.perspective.SchedulingAnalysis"
            name="Scheduling Analysis">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="edu.tum.cs.cadmos.analysis.ui.perspective.SchedulingAnalysisMainEditor"
            id="edu.tum.cs.cadmos.analysis.ui.perspective.SchedulingAnalysisMainEditor"
            name="New EditorPart">
      </editor>
      <editor
            class="edu.tum.cs.cadmos.analysis.ui.perspective.SchedulingAnalysisEditorContainer"
            id="edu.tum.cs.cadmos.analysis.ui.perspective.SchedulingAnalysisEditorContainer"
            name="SchedulingAnalysisEditorContainer">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="edu.tum.cs.cadmos.analysis.ui.schedule.ScheduleSoftwareHandler"
            commandId="edu.tum.cs.cadmos.analysis.ui.ScheduleSoftwareHandler">
      </handler>
      <handler
            class="edu.tum.cs.cadmos.analysis.ui.schedule.ScheduleProcessingHandler"
            commandId="edu.tum.cs.cadmos.analysis.ui.ScheduleProcessingHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:#TextEditorContext?after=additions">
         <command
               commandId="edu.tum.cs.cadmos.analysis.ui.ScheduleSoftwareHandler"
               label="Schedule Software Component"
               style="toggle"
               tooltip="Enques the software component for scheduling.">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:#TextEditorContext?after=additions">
         <command
               commandId="edu.tum.cs.cadmos.analysis.ui.ScheduleProcessingHandler"
               label="Schedule Processing Component"
               style="toggle"
               tooltip="Enques the processing component for scheduling.">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="edu.tum.cs.cadmos.analysis.ui.ScheduleSoftwareHandler"
            name="Schedule Software">
         <state
               class="org.eclipse.ui.handlers.RegistryToggleState:false "
               id="edu.tum.cs.cadmos.analysis.ui.ScheduleSoftwareHandlerState">
         </state>
      </command>
      <command
            id="edu.tum.cs.cadmos.analysis.ui.ScheduleProcessingHandler"
            name="Schedule Processing">
         <state
               class="org.eclipse.ui.handlers.RegistryToggleState:false "
               id="edu.tum.cs.cadmos.analysis.ui.ScheduleProcessingHandlerState">
         </state>
      </command>
   </extension>
</plugin>
