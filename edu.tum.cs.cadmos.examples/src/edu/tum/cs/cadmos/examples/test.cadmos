package test


software component Container {
	in input
	out output
	
	embed SubComponent subA
	embed SubComponent subB
	
	channel input -> subA.sinput
	channel subA.soutput -> subB.sinput
	channel subB.soutput -> output
	
}

software component SubComponent {
	in sinput
	out soutput
	
	embed SubSubComponent ssubA
	embed SubSubComponent ssubB
	channel sinput -> ssubA.ssinput
	channel ssubA.ssoutput -> ssubB.ssinput
	channel ssubB.ssoutput -> soutput
}
software component SubSubComponent {
	in ssinput
	out ssoutput
}

